{
  "framework": "nextjs",
  "regions": ["iad1"],
  "buildCommand": "cd project-cobra && npm run build",
  "installCommand": "cd project-cobra && npm install",
  "outputDirectory": "project-cobra/.next",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 